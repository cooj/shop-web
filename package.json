{
    "private": true,
    "packageManager": "pnpm@7.29.3",
    "scripts": {
        "build": "nuxi build",
        "dev": "nuxi dev",
        "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
        "start": "node .output/server/index.mjs",
        "type-check": "vue-tsc --noEmit",
        "lint": "eslint . --fix",
        "postinstall": "nuxi prepare",
        "generate": "nuxi generate",
        "start:generate": "npx serve .output/public",
        "prepare": "husky install",
        "commit": "git-cz"
    },
    "dependencies": {
        "big.js": "^6.2.1",
        "element-plus": "^2.3.5",
        "qrcode": "^1.5.3",
        "swiper": "^9.3.2",
        "vue-tsc": "^1.6.5"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^0.39.0",
        "@element-plus/nuxt": "^1.0.5",
        "@iconify-json/carbon": "^1.1.16",
        "@iconify-json/ep": "^1.1.10",
        "@iconify-json/ic": "^1.1.13",
        "@iconify-json/twemoji": "^1.1.11",
        "@nuxt/devtools": "^0.5.0",
        "@nuxtjs/color-mode": "^3.2.0",
        "@pinia/nuxt": "^0.4.11",
        "@types/big.js": "^6.1.6",
        "@types/qrcode": "^1.5.0",
        "@unocss/eslint-config": "^0.51.13",
        "@unocss/nuxt": "^0.51.13",
        "@unocss/preset-rem-to-px": "^0.51.13",
        "@vite-pwa/nuxt": "^0.0.8",
        "@vueuse/nuxt": "^10.1.2",
        "commitizen": "^4.3.0",
        "cz-git": "^1.6.1",
        "eslint": "^8.41.0",
        "husky": "^8.0.3",
        "lint-staged": "^13.2.2",
        "nuxt": "^3.5.0",
        "pinia": "^2.1.3",
        "sass": "^1.62.1",
        "typescript": "^5.0.4"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-git"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged0": {
        "*.vue": [
            "eslint --fix"
        ],
        "*.{htm,html,css,sss,less,scss,sass}": [
            "eslint --fix"
        ]
    },
    "lint-staged": {
        "*.vue": [
            "eslint --fix"
        ],
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix"
        ]
    }
}