    "prettier.enable": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "editor.formatOnSave": true,
    "editor.tabSize": 4,
    "files.associations": {
        "*.css": "postcss"
    },
    "cSpell.words": [
