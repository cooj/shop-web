        "antfu",
        "Attributify",
        "backtop",
        "breakline",
        "cascader",
        "codeid",
        "composables",
        "iconify",
        "nuxi",
        "nuxt",
        "nuxtjs",
        "parentid",
        "pinia",
        "pnpm",
        "qrcode",
        "recom",
        "stylelint",
        "swiper",
        "Uno",
        "unocss",
        "Vite",
        "vueuse"
    ]
}
